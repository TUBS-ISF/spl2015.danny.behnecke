					<feature name="readSet"/>
				</and>
				<feature name="xml"/>
				<feature name="json"/>
			</and>
		</struct>
		<constraints>
			<rule>
				<imp>
					<var>set</var>
					<var>variable</var>
				</imp>
			</rule>
			<rule>
				<imp>
					<var>customDistribution</var>
					<var>normalDistributed</var>
				</imp>
			</rule>
			<rule>
				<imp>
					<var>saveSet</var>
					<disj>
						<var>json</var>
						<var>xml</var>
					</disj>
				</imp>
			</rule>
			<rule>
				<imp>
					<var>loadSet</var>
					<disj>
						<var>json</var>
						<var>xml</var>
					</disj>
				</imp>
			</rule>
			<rule>
				<imp>
					<var>histogram</var>
					<var>plot</var>
				</imp>
			</rule>
			<rule>
				<imp>
					<conj>
						<var>histogram</var>
						<conj>
							<var>scatterplot</var>
							<conj>
								<var>table</var>
								<var>plot</var>
							</conj>
						</conj>
					</conj>
					<disj>
						<var>Terminal</var>
						<var>GUI</var>
					</disj>
				</imp>
			</rule>
		</constraints>
		<calculations Auto="true" Constraints="true" Features="true" Redundant="true" Tautology="true"/>
		<comments/>
		<featureOrder userDefined="false"/>
